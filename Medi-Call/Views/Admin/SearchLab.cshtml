@model IEnumerable<Medi_Call.Models.Lab>

<p>
    @Html.ActionLink("Create New", "Register","Admin")
</p>
<p>
    @using (Html.BeginForm("SearchLab", "Admin", FormMethod.Get))
    {
        @Html.TextBox("search") <input type="submit" value="Search" />
    }
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
    
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
     
      
        <td>
     
            @Html.ActionLink("Details", "LDetails", new { id=item.Name }) |
            @Html.ActionLink("Delete", "DeleteLab", new { id=item.Name })
        </td>
    </tr>
}

</table>
